<Window x:Class="DesignPatterns.SecondPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesignPatterns"
        mc:Ignorable="d"
        Title="SecondPage" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <TextBox Name="txtConstant" Margin="2"  Grid.Row="0"/>
        <Button Name="btnSaveValue" Content="Save Value" Click="btnSaveValue_Click" Grid.Row="1" Margin="2"/>
    </Grid>
</Window>
